<div class="container my-5 ">
    <div class="row">
        <div class="col-md-8 mx-auto">
            <a class="btn btn-primary d-inline" href="/create">Add new student</a>
            <br>
            <br>
            <div class="card">
                <div class="card-body">
                    <h1>All Student</h1>
                    <hr>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>
                                <th>Cell</th>
                                <th>Email</th>
                                <th>Photo</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% students.forEach((element, index) => { %>
                                <tr>
                                    <td><%= index+1 %> </td>
                                    <td><%= element.name %> </td>
                                    <td><%= element.cell %></td>
                                    <td><%= element.email %></td>
                                    <td>
                                        <img style="width: 50px; height: 50px; object-fit: cover;" src="upload/images/<%= element.photo %> " alt="">
                                    </td>
                                    <td>
                                        <a href="/view/<%= element._id %>"><i class="btn btn-sm btn-success fas fa-eye"></i></a>
                                        <a href="/edit/<%= element._id %> "><i class="btn btn-sm btn-info fas fa-edit"></i></a>
                                        <a name="_mathod" value="delete" href="/delete/<%= element._id %> "><i class="btn btn-sm btn-danger fas fa-trash"></i></a>
                                    </td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>